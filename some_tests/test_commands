POST /kv body: {key: test, "value" : { "key" : "value"}}
POST /kv body: {key test, "value" : { "key" : "value"}}
POST /kv body: { test, "value" : { "key" : "value"}}


///incorrect value
POST /kv body: {key: "test", value" : { "key" : "value"}}
POST /kv body: {key: "test", "value : { "key" : "value"}}
POST /kv body: {key: "test", "value: { "key" : "value"}}
POST /kv body: {key: "test", value: { "key" : "value"}}
POST /kv body: {key: "test", "value : { "key" : "value"}
POST /kv body: key: "test", "value : { "key" : "value"}

